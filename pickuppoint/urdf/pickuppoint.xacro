        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0.00 0.395 0" />
            <geometry>
                <mesh filename="package://pickuppoint/stl/PickupPoint.stl" />
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0.0 0.395 0" />
            <geometry>
                <mesh filename="package://pickuppoint/stl/ultimateLowResPickupPoint3.0.stl" />
            </geometry>
            <material name="">
                <color rgba="1 1 0 1" />
