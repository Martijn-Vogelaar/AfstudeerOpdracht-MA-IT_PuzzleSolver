<?xml version="1.0" ?>
<robot name="pickuppoint" 
    xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro" />
    <link name='world'/>
    <link name='base_link'>
    <inertial>
      <mass value="100" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
        <visual name='visual'>
            <origin rpy="0 0 0" xyz="0.025 0.395 0" />
            <geometry>
                <mesh filename="package://pickuppoint/stl/lowResPickupPoint.stl" />
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0.025 0.395 0" />
            <geometry>
                <mesh filename="package://pickuppoint/stl/lowResPickupPoint.stl" />
            </geometry>
            <material name="">
                <color rgba="1 1 0 1" />
            </material>
        </collision>
    </link>
    <link name='link_1'>
        <inertial>
            <mass value="1"/>
            <origin rpy="0 0 -1.57" xyz="0 0 0" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001" />
        </inertial>
        <visual name='visual'>
            <origin rpy="0 0 -1.57" xyz="-0.39 0.02 0.28" />
            <geometry>
                <box size="0.4 0.1 0.02"/>
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 -1.57" xyz="-0.39 0.02 0.28" />
            <geometry>
                <box size="0.4 0.1 0.02"/>
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </collision>
    </link>
    <link name='link_2'>
        <inertial>
            <mass value="1"/>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001" />
        </inertial>
        <visual name='visual'>
            <origin rpy="0 0 0" xyz="0 -0.27 0.273" />
            <geometry>
                <box size="0.26 0.36 0.002"/>
            </geometry>
            <material name="Green">
                <color rgba="0 1.0 0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 -0.27 0.273" />
            <geometry>
                <box size="0.26 0.36 0.002"/>
            </geometry>
            <material name="Green">
                <color rgba="0 1.0 0 1.0"/>
            </material>
        </collision>
    </link>
    <joint name="base_link_JOINT_1" type="prismatic">
        <origin rpy="0 0 1.57" xyz="0 0 0" />
        <parent link="base_link" />
        <child link="link_1" />
        <limit effort="310" lower="0" upper="0.23" velocity="0.1" />
    </joint>
    <joint name="world_base" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="world" />
        <child link="base_link" />
    </joint>

     <joint name="base_link2" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="base_link" />
        <child link="link_2" />
    </joint>
    <!-- </xacro:macro> -->
</robot>