<?xml version="1.0" ?>
<robot name="pickuppoint" 
    xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro" />
    <link name='world'/>
    <gazebo reference="base_link">
        <material>Gazebo/Yellow</material>
